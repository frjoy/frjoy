{
  "name": "dev",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "build:package": "parcel build 'packages/*/' --no-cache",
    "build:fix-type-defs": "node ./scripts/fix-type-defs-imports",
    "release": "pnpm build:package && changeset publish",
    "clean": "pnpm  -r clean",
    "reset": "pnpm clean && rm -rf node_modules .parcel-cache",
    "bump:stable": "pnpm version -r",
    "bump:next": "pnpm version -r --prerelease",
    "bump:check": "pnpm version check",
    "changeset:version": "changeset version && pnpm copy:changelog && rm -rf .changeset/*.md",
    "copy:changelog": "rimraf .storybook/docs/CHANGELOG.mdx && copyfiles CHANGELOG.md  .storybook/docs && rename .storybook/docs/CHANGELOG.md CHANGELOG.mdx && rimraf .storybook/docs/CHANGELOG.md"
  },
  "keywords": [],
  "author": "Felix Dusengimana <info@feldux.com>",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.27.2",
    "@chromatic-com/storybook": "^1.4.0",
    "@parcel/packager-ts": "2.12.0",
    "@parcel/transformer-typescript-types": "2.12.0",
    "@storybook/addon-essentials": "^8.1.1",
    "@storybook/addon-interactions": "^8.1.1",
    "@storybook/addon-links": "^8.1.1",
    "@storybook/addon-onboarding": "^8.1.1",
    "@storybook/blocks": "^8.1.1",
    "@storybook/manager-api": "^8.1.1",
    "@storybook/react": "^8.1.1",
    "@storybook/react-vite": "^8.1.1",
    "@storybook/test": "^8.1.1",
    "@storybook/theming": "^8.1.1",
    "@testing-library/react": "^15.0.7",
    "@types/fs-extra": "^11.0.4",
    "bump": "^0.2.5",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.15",
    "jsdom": "^24.0.0",
    "parcel": "^2.12.0",
    "prop-types": "^15.8.1",
    "replace-in-files": "^3.0.0",
    "rimraf": "^5.0.7",
    "storybook": "^8.1.1",
    "typescript": ">=3.0.0",
    "vitest": "^1.6.0"
  }
}
